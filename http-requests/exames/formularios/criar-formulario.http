### Criar formulário de anamnese
POST {{baseUrl}}/formularios
Content-Type: {{contentType}}
Authorization: Bearer {{token}}

{
  "codigoFormulario": "ANAM_001",
  "nomeFormulario": "Formulário de Anamnese Geral",
  "descricao": "Formulário padrão para coleta de dados de anamnese de pacientes",
  "tipo": "anamnese",
  "categoria": "clinico",
  "versao": 1,
  "unidadeSaudeId": "{{userId}}",
  "tags": ["anamnese", "clinico", "geral"],
  "configuracoes": {
    "permitirSalvarParcial": true,
    "exigirAssinaturaDigital": false,
    "tempoLimiteMinutos": 60,
    "notificarProfissional": true
  },
  "metadados": {
    "autor": "Dr. João Silva",
    "especialidade": "Clínica Geral",
    "versaoMinimaSistema": "1.0.0"
  },
  "observacoes": "Formulário padrão para primeira consulta"
}

