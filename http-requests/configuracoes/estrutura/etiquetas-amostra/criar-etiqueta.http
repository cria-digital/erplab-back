### Criar Etiqueta - Sangue Tubo Pequeno
POST {{baseUrl}}/configuracoes/estrutura/etiquetas-amostra
Content-Type: {{contentType}}
Authorization: Bearer {{token}}

{
  "codigoEtiqueta": "ETQ-SANG-001",
  "nome": "Etiqueta Sangue - Tubo Pequeno",
  "tipoAmostra": "sangue",
  "largura": 40,
  "altura": 20,
  "orientacao": "paisagem",
  "layout": {
    "tipo": "grid",
    "colunas": 2,
    "linhas": 3,
    "espacamento": 2
  },
  "campos": ["codigoBarras", "nomeParaciente", "dataColeta", "horaColeta"],
  "corPrimaria": "#FF0000",
  "corSecundaria": "#FFFFFF",
  "templateZPL": "^XA^FO50,50^BCN,100,Y,N,N^FD{codigo}^FS^FO50,170^A0N,25,25^FD{paciente}^FS^XZ",
  "padrao": true,
  "ativo": true,
  "observacoes": "Etiqueta padrão para tubos de sangue pequenos (3-5mL)"
}

### Expected Response
# Status: 201 Created
# {
#   "id": "uuid",
